{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "no-use-before-define": "warn",
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "function-paren-newline": 0
  }
}
